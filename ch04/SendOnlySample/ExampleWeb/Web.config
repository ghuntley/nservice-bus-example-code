<configuration>
	<configSections>
		<section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core" />
	</configSections>
    
    <!-- Since this is a send-only endpoint, MessageForwardingInCaseOfFaultConfig is not needed. -->

